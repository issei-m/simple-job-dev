{
    "name": "issei-m/simple-job-queue",
    "type": "library",
    "description": "The simple job queue and worker for PHP.",
    "keywords": ["job", "queue", "worker", "symfony", "process"],
    "homepage": "https://github.com/issei-m/simple-job-queue",
    "license": "MIT",
    "authors": [
        {
            "name": "Issei Murasawa",
            "email": "issei.m7@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-pcntl": "*",
        "doctrine/instantiator": "^1.1",
        "ramsey/uuid": "^3.7",
        "symfony/process": "^2.7 || ^3.0"
    },
    "require-dev": {
        "doctrine/dbal": "^2.6",
        "monolog/monolog": "^1.23",
        "predis/predis": "^1.1",
        "symfony/console": "^3.3",
        "symfony/var-dumper": "^2.7 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "Issei\\SimpleJobQueue\\": "./"
        }
    },
    "config": {
        "platform": {
            "php": "7.1"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
